<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.goovy.portfolioapp.gui.about.AboutView">
    <children>
        <Label layoutX="20" layoutY="20" styleClass="about-title" text="%about.title">
            <font>
                <Font name="Arial" size="20" />
            </font>
        </Label>
        <Label layoutX="20" layoutY="60" styleClass="about-text" fx:id="version">
            <font>
                <Font name="Arial" size="12" />
            </font>
        </Label>
      <Button onAction="#close" layoutX="344.0" layoutY="201.0" mnemonicParsing="false" text="%button.close" />

    </children>

</AnchorPane>
